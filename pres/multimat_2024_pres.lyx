\begin_layout Frame
\begin_inset Formula 
\begin{align*}
\text{mass} &  & \frac{D\rho}{Dt} & =-\rho\partial_{x}^{\alpha}v^{\alpha},\\
\text{momentum} &  & \rho\frac{Dv^{\alpha}}{Dt} & =-\partial_{x}^{\alpha}p-{\color{col1}\sum_{g}\lambda_{g}\partial_{x}^{\alpha}E_{g}},\\
\text{material energy} &  & \rho\frac{De}{Dt} & =-\partial_{x}^{\alpha}pv^{\alpha}-\sum_{g}\left({\color{col2}c\sigma_{a,g}B_{g}}+{\color{col3}c\sigma_{a,g}E_{g}}\right)+Q_{e},\\
\text{radiation energy} &  & {\color{col4}\frac{DE_{g}}{Dt}} & =-{\color{col5}\frac{4}{3}E_{g}\partial_{x}^{\alpha}v^{\alpha}}+\partial_{x}^{\alpha}\frac{c\lambda_{g}}{\sigma_{t,g}}\partial_{x}^{\beta}E-{\color{col3}c\sigma_{a,g}E_{g}}+{\color{col2}c\sigma_{a,g}B_{g}}+Q_{E,g},
