\begin{align*}
f\left(x\right) & =\int_{V}\delta\left(x-x'\right)f\left(x'\right)dV'\\
 & \approx\int_{V}W\left(x-x',h\right)f\left(x'\right)dV'\\
 & \approx\sum_{j}V_{j}W\left(x-x_{j},h\right)f_{j}
\end{align*}

\end_inset


\end_layout

\end_inset


\end_layout

